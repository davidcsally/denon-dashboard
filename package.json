{
  "name": "denon-dashboard",
  "version": "1.0.0",
  "description": "a dashboard and api for controlling your denon receiver",
  "main": "index.js",
  "scripts": {
    "build": "node scripts/deletePublic && SET NODE_ENV=production&& webpack --progress --colors --config webpack.config.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm-run-all -p start:client start:api",
    "start:client": "webpack -w",
    "start:api": "node node_modules/restful-denon/server",
    "on": "node scripts/turnOn.js",
    "test": "test"
  },
